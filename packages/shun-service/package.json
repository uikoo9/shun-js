{
  "name": "@shun-js/shun-service",
  "version": "0.1.0",
  "description": "shun.js service",
  "keywords": [
    "shun.js",
    "service"
  ],
  "license": "MIT",
  "author": "uikoo9 <uikoo9@qq.com>",
  "homepage": "https://github.com/uikoo9/shun-js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/uikoo9/shun-js.git"
  },
  "bugs": {
    "url": "https://github.com/uikoo9/shun-js/issues"
  },
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rm -rf dist && qpro rollup ./rollup.config.js"
  },
  "dependencies": {
    "qiao-ajax": "^5.2.4",
    "qiao-json": "^5.1.9",
    "qiao.log.js": "^3.7.5"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "nx": {
    "namedInputs": {
      "default": [
        "{projectRoot}/src/**/*"
      ]
    },
    "targets": {
      "build": {
        "inputs": [
          "default"
        ],
        "outputs": [
          "{projectRoot}/dist"
        ]
      }
    }
  }
}
